<%= form_for([@school, @school_administration]) do |f| %>

  <%= render 'shared/error_messages', resource: @school_administration %>

  <div class="field">
    <%= label_tag :email %><br>
    <%= email_field_tag :email %>
  </div>
  <div class="actions">
    <%= f.submit 'Submit' %>
  </div>
<% end %>
