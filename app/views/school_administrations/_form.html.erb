<%= simple_form_for([@school, @school_administration]) do |f| %>
  <%= render 'shared/error_messages', resource: @school_administration %>

  <div class="form-inputs">
    <%= label_tag :email, nil, class: 'control-label' %><br>
    <%= email_field_tag :email, nil, class: 'form-control' %>
  </div>
  <div class="form-actions">
    <%= f.button :submit, 'Submit' %>
  </div>
<% end %>
